{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true /* Disallow inconsistently-cased references to the same file. */ ,
    "watch": true,
    "sourceMap": true
  },
  "exclude": [
    "node_modules"
  ]
}